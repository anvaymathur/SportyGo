{
  "expo": {
     "name": "SportyGo",
    "slug": "SportyGo",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sportygo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sparkpro.sportygo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sparkpro.sportygo",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-auth0",
        {
          "domain": "sportygo.ca.auth0.com",
          "clientId": "TzevmHUuWdBTRK9jmAkswj4bBAUyvovB",
          "packageName": "com.sparkpro.sportygo"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "manifestPlaceholders": {
              "auth0Domain": "sportygo.ca.auth0.com",
              "auth0Scheme": "com.sparkpro.sportygo.auth0"
            },
            "gradleVersion": "8.6",
            "androidGradlePluginVersion": "8.3.2"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "arav_mathur",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7f41d17-0883-408b-a1b0-ae9f088e7796"
      }
    }
  }
}
