{
  "expo": {
    "name": "BadmintonApp",
    "slug": "BadmintonApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "badmintonapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aravmathur.badmintonapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aravmathur.badmintonapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-auth0",
        {
          "domain": "dev-6oulj204w6qwe4dk.us.auth0.com",
          "clientId": "CHYISQSTA3BStcmW4Yrz9ThiLV0PFUdv",
          "packageName": "com.aravmathur.badmintonapp"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "manifestPlaceholders": {
              "auth0Domain": "dev-6oulj204w6qwe4dk.us.auth0.com",
              "auth0Scheme": "com.aravmathur.badmintonapp.auth0"
            }
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "924db136-fd31-46f2-af62-994f01ed1b96"
      }
    }
  }
}
